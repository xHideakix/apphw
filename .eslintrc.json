{
  "extends": "eslint:recommended",
  "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
      }
  },
  "rules": {
      "semi": "error",
      "no-unused-vars": ["error", { "varsIgnorePattern": "Click$" }],
      "indent": ["error", 2],
      "quotes": ["error", "single"],
      "quote-props": ["error", "as-needed"]
  }
}